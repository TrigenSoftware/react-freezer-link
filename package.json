{
  "name": "react-freezer-link",
  "version": "2.0.0",
  "description": "A React mixin for linking form fields to a deep structure of data within the component's props which provided by Freezer.",
  "main": "lib/mixin.js",
  "scripts": {
    "compile": "babel ./src -d ./lib",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/react-freezer-link.git"
  },
  "keywords": [
    "react",
    "link",
    "props",
    "mixin",
    "deep",
    "freezer",
    "linkstate",
    "flux",
    "tree",
    "form"
  ],
  "author": "dangreen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrigenSoftware/react-freezer-link/issues"
  },
  "homepage": "https://github.com/TrigenSoftware/react-freezer-link#readme",
  "dependencies": {
    "lodash": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-plugin-add-module-exports": "^0.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
